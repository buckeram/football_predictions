{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Action": [
        "logs:CreateLogGroup",
        "logs:CreateLogStream",
        "logs:PutLogEvents"
      ],
      "Resource": "arn:aws:logs:*:*:*",
      "Effect": "Allow"
    },
    {
      "Action": [
         "s3:PutObject",
         "s3:GetObject"
      ],
      "Resource": "arn:aws:s3:::alanbuckeridge*",
      "Effect": "Allow"
    },
    {
      "Action": [
         "iam:PassRole"
      ],
      "Resource": "arn:aws:iam::*:role/media-query-*",
      "Effect": "Allow"
    }
  ]
}
